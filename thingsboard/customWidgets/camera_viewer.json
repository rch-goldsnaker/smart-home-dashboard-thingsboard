    "templateHtml": "<div class=\"parent-container\">\r\n    <div class=\"header\">\r\n        <div>\r\n            <h3>{{title}}</h3>\r\n            <p>{{subtitle}}</p>\r\n        </div>\r\n        <div class=\"form-check form-switch switch\">\r\n            <input class=\"form-check-input\" type=\"checkbox\"\r\n                role=\"switch\" id=\"flexSwitchCheckDefault\"\r\n                checked=\"true\"\r\n                (change)=\"gpioToggleChange($event)\">\r\n        </div>\r\n    </div>\r\n    <div class=\"iframe-wrapper\">\r\n        <iframe *ngIf=\"iframeVisible\"\r\n            class=\"responsive-iframe\"\r\n            src=\"https://rtsp.me/embed/faN3T75N/?\"\r\n            frameborder=\"0\" allowfullscreen></iframe>\r\n        <div class=\"hidden\" *ngIf=\"!iframeVisible\">\r\n            <i class=\"bi bi-eye-slash-fill icon\"></i>\r\n        </div>\r\n    </div>\r\n</div>",
    "templateCss": "h3{\n    margin:0px;\n    padding: 0px;\n    font-size: 1.2rem;\n    font-weight: 500;\n}\n\np{\n    margin:0px;\n    padding: 0px;\n    font-size: 0.8rem;\n    font-weight: 500;\n    color:#B6C3DB;\n}\n\n.header{\n    display: flex;\n    justify-content: space-between;\n}\n\n.parent-container {\n    display: flex;\n    flex-direction: column;\n    height: 100%; \n}\n\n.iframe-wrapper {\n    flex: 1 1 auto;\n    display: flex;\n    margin-top: 10px;\n}\n\n.responsive-iframe {\n    flex: 1 1 auto;\n    width: 100%;\n    height: 100%;\n    border: 0;\n}\n\n.hidden{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    background: #F6F7F9;\n}\n\n.icon{\n    color: #3E86F6;\n    font-size: 2rem;\n}\n\n.switch{\n    font-size: 1.5rem;\n}\n",
    "controllerScript": "self.onInit = function() {\r\n\r\n    self.ctx.$scope.title = self.ctx.settings\r\n        .title;\r\n    self.ctx.$scope.subtitle = self.ctx.settings\r\n        .subtitle;\r\n    self.ctx.$scope.sourceCam = self.ctx.settings.sourceCam;\r\n    \r\n    self.ctx.$scope.iframeVisible = false;\r\n\r\n    if (!self.ctx.$scope.title) {\r\n        self.ctx.$scope.title = \"Title\";\r\n    }\r\n    if (!self.ctx.$scope.subtitle) {\r\n        self.ctx.$scope.subtitle =\r\n            \"Subtitle\";\r\n    }\r\n    \r\n    self.ctx.$scope.gpioToggleChange = function($event) {\r\n        const target = event.target;\r\n        // console.log(target.checked);\r\n        self.ctx.$scope.iframeVisible = target.checked;\r\n        self.ctx.detectChanges();\r\n    };\r\n    \r\n    \r\n}\r\n",
