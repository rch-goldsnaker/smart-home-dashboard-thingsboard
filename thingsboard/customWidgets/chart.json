    "templateCss": ".main-widget {\n    display: flex;\n    flex-direction: column;\n    height: 100%;\n}\n\n.title {\n    margin: 0px;\n    padding: 0px;\n    font-size: 1.2rem;\n    font-weight: 500;\n}\n\n.subtitle {\n    margin: 0px;\n    padding: 0px;\n    font-size: 0.8rem;\n    font-weight: 500;\n    color: #B6C3DB;\n}\n\n.header {\n    display: flex;\n    align-items: center;\n}\n\n.canva-wrapper {\n    flex-grow: 1;\n    display: flex;\n    width: 100%;\n    height: 80%;\n    margin-top: 10px;\n}\n\n.responsive-canva {\n    flex: 1 1 auto;\n    width: 100%;\n    height: 100%;\n    border: 0;\n}",
    "controllerScript": "var myChart;\r\n\r\nself.onInit = function() {\r\n    \r\n    const { $scope, settings } = self.ctx;\r\n\r\n    $scope.title = settings.title || \"Title\";\r\n    $scope.subtitle = settings.subtitle || \"Subtitle\";\r\n    $scope.checkState = false;\r\n    \r\n    \r\n    const chartData = {\r\n        datasets: self.ctx.data.map(dataItem => ({\r\n            label: dataItem.dataKey.label,\r\n            data: [],\r\n            borderColor: dataItem.dataKey.color,\r\n            fill: false\r\n        }))\r\n    };\r\n\r\n    const options = {\r\n        maintainAspectRatio: false,\r\n        legend: { display: false },\r\n        scales: {\r\n            xAxes: [{\r\n                type: 'time',\r\n                ticks: {\r\n                    maxRotation: 0,\r\n                    autoSkipPadding: 30,\r\n                    fontColor: '#B6C3DB'\r\n                }\r\n            }],\r\n            yAxes: [{\r\n                ticks: {\r\n                    fontColor: '#B6C3DB'\r\n                }\r\n            }]\r\n        }\r\n    };\r\n\r\n    const canvasElement = $('#myChart', self.ctx.$container)[0];\r\n    const canvasCtx = canvasElement.getContext('2d');\r\n    myChart = new Chart(canvasCtx, {\r\n        type: 'line',\r\n        data: chartData,\r\n        options: options\r\n    });\r\n\r\n    self.onResize();\r\n};\r\n\r\nself.onResize = function() {\r\n    if (myChart) {\r\n        myChart.resize();\r\n    }\r\n    self.ctx.detectChanges();\r\n};\r\n\r\nself.onDataUpdated = function() {\r\n    self.ctx.data.forEach((dataItem, i) => {\r\n        const dataset = myChart.data.datasets[i];\r\n        dataset.data.length = 0;\r\n        dataset.data.push(...dataItem.data.map(tsValuePair => ({\r\n            t: tsValuePair[0],\r\n            y: tsValuePair[1]\r\n        })));\r\n    });\r\n\r\n    const xAxis = myChart.options.scales.xAxes[0].ticks;\r\n    xAxis.min = self.ctx.timeWindow.minTime;\r\n    xAxis.max = self.ctx.timeWindow.maxTime;\r\n\r\n    myChart.update();\r\n};\r\n",
    "settingsSchema": "{\r\n    \"schema\": {\r\n        \"type\": \"object\",\r\n        \"title\": \"Settings\",\r\n        \"properties\": {\r\n            \"title\": {\r\n                \"title\": \"Title\",\r\n                \"type\": \"string\",\r\n                \"default\": \"Title\"\r\n            },\r\n            \"subtitle\": {\r\n                \"title\": \"Subtitle\",\r\n                \"type\": \"string\",\r\n                \"default\": \"Subtitle\"\r\n            }\r\n        },\r\n        \"required\": []\r\n    },\r\n    \"form\": [\r\n        \"title\",\r\n        \"subtitle\"\r\n    ]\r\n}",
